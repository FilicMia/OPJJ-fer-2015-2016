package hr.fer.zemris.java.tecaj.hw5.views;

import java.util.List;

import hr.fer.zemris.java.tecaj.hw5.db.StudentRecord;

/**
 * Interface for classes that are responsible for visualization of
 * images.
 * 
 * @author mia
 *
 */
public interface View {

	/**
	 * Creates the {@code String} representation of records.
	 * @param raster
	 *            raster whose picture/image will produce
	 * @return produced object
	 */
	public Object produce(List<StudentRecord> records);

}
